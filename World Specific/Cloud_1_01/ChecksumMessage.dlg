}
terminal when (Randomizer_Overlay_Hex_F) {
  text: "TTRS:notrans=F%e2"
}

terminal when (Randomizer_Overlay_End) {
  text: [[TTRS:notrans=
%w9%w9%w9%w9%e2]]
}
